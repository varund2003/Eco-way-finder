					// 	path: pp.latlngs,
					// 	// strokeColor: pp.colour,
					// 	strokeColor: pp.opacities.map(fraction => pSBC(fraction, pp.colour, 'rgb(255,255,255)')),
					// 	// strokeOpacity: ((pp.opacities[i] * 4) + 1) / 5,
					// 	strokeOpacity: pp.opacities,
					// 	// strokeOpacity: 1,
					// 	// strokeWeight: (pp.opacities[i] * 6) + 2,
					// 	strokeWeight: 7,
					// 	map: me.map
